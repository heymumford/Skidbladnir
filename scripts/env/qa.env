# QA environment configuration
ENVIRONMENT=qa
NODE_ENV=production
LOG_LEVEL=debug

# API configuration
API_PORT=3000
API_HOST=0.0.0.0
API_BASE_URL=https://qa-api.skidbladnir.example.com

# Orchestrator configuration
ORCHESTRATOR_PORT=8000
ORCHESTRATOR_HOST=0.0.0.0
ORCHESTRATOR_API_URL=https://qa-orchestrator.skidbladnir.example.com

# Binary processor configuration
BINARY_PROCESSOR_PORT=9000
BINARY_PROCESSOR_HOST=0.0.0.0

# Database configuration
DB_HOST=qa-db.skidbladnir.example.com
DB_PORT=5432
DB_NAME=skidbladnir_qa
DB_USER=skidbladnir
DB_PASSWORD=

# Cache configuration
REDIS_HOST=qa-cache.skidbladnir.example.com
REDIS_PORT=6379

# Storage configuration
STORAGE_TYPE=s3
S3_BUCKET=skidbladnir-qa
S3_REGION=us-west-2

# LLM configuration
LLM_MODEL_PATH=/app/llm-models/llama-3-8b-q4.gguf
LLM_THREADS=4
LLM_CONTEXT_SIZE=4096
LLM_BATCH_SIZE=512